public class Practs6 {
    void add(int a, int b) {
        int c = a + b;
        System.out.println("Addition of 2 nos = " + c);
    }

    void add(int a, int b, int c) {
        int d = a + b + c;
        System.out.println("Addition of 3 nos = " + d);
    }

    public static void main(String args[]) {
        Practs6 p = new Practs6();
        p.add(4, 5);
        p.add(7, 8, 4);
    }
}
