import java.util.Scanner;

class table {
    public static void main(String args[]) {
        int a, b;
        Scanner s = new Scanner(System.in);
        System.out.println("Enter a number: ");
        a = s.nextInt();
        for (b = 1; b <= 10; b++) {
            System.out.println(a + " x " + b + " = " + (a * b));
        }
    }
}
